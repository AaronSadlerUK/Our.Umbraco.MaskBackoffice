{
  "$schema" : "./umbraco/config/appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "Console"
            }
          ]
        }
      }
    ]
  },
  "Umbraco": {
    "CMS": {
      "Content": {
        "MacroErrors": "Throw",
        "Error404Collection": [
          {
            "Culture": "default",
            "ContentKey": "1d770f10-d1ca-4a26-9d68-071e2c9f7ac1"
          }
        ]
      },
      "Global": {
        "Smtp": {
          "From": "your@email.here",
          "Host": "localhost",
          "Port": 25
        }
      },
      "Hosting": {
        "Debug": true
      },
      "RuntimeMinification": {
        "useInMemoryCache": true,
        "cacheBuster": "Timestamp"
      }
    }
  },
  "OurUmbracoMaskBackoffice": {
    "Enabled": true,
    "UseRedirect": true,
    "RedirectUrl": "/contact/",
    "Domains": [
      "google.com"
    ]
  }
}